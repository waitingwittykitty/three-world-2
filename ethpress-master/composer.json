{
    "name": "losnappas/ethpress",
    "description": "Log in to WordPress with a cryptocurrency wallet via MetaMask browser extension.",
    "keywords": ["wordpress", "plugin"],
    "version": "0.0.1",
    "homepage": "https://gitlab.com/losnappas/ethpress",
    "license": "GPL-3.0+",
    "authors": [{
        "name": "losnappas",
        "email": "lynn.mvp@tutanota.com",
        "homepage": "https://gitlab.com/losnappas"
    }],
    "type": "wordpress-plugin",
    "autoload": {
        "psr-4":{
            "losnappas\\Ethpress\\": "app/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/devoldemar/keccak"
        }
    ],
    "require-dev": {
        "coenjacobs/mozart": "dev-master"
    },
    "require": {
        "devoldemar/keccak": "master",
        "simplito/elliptic-php": "dev-master"
    },
    "minimum-stability": "dev",
    "extra": {
        "mozart": {
            "dep_namespace": "losnappas\\Ethpress\\Dependencies\\",
            "dep_directory": "/app/Dependencies/",
            "classmap_directory": "/app/Dependencies/__classmaps/",
            "classmap_prefix": "Ethpress_",
            "packages": [
                "devoldemar/keccak",
                "simplito/elliptic-php"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "\"vendor/bin/mozart\" compose",
            "composer dump-autoload"
        ],
        "post-update-cmd": [
            "\"vendor/bin/mozart\" compose",
            "composer dump-autoload"
        ],
        "build": [
            "rm -rf vendor",
            "composer dump-autoload -o"
        ]
    }
}
