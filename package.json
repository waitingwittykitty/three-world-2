{
    "name": "@companyjuice/threed-garden",
    "version": "0.7.6",
    "description": "ThreeD Garden WP Plugin on Vue 3, using Vite with WindiCSS, and Farmbot JS",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/companyjuice/threedgarden.git"
    },
    "author": "Marty McGee, Company Juice",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/companyjuice/threedgarden/issues"
    },
    "homepage": "https://github.com/companyjuice/threedgarden#readme",
    "private": true,
    "scripts": {
        "react-app:build": "yarn workspace @threed/react-app build --max-old-space-size=12288",
        "react-app:eject": "yarn workspace @threed/react-app eject",
        "react-app:start": "yarn workspace @threed/react-app start",
        "react-app:test": "yarn workspace @threed/react-app test",
        "build": "yarn workspace @threed/react-app build --max-old-space-size=12288",
        "prettier": "yarn workspace @threed/react-app prettier",
        "chain": "yarn workspace @threed/hardhat chain",
        "fork": "yarn workspace @threed/hardhat fork",
        "node": "yarn workspace @threed/hardhat chain",
        "test": "yarn workspace @threed/hardhat test",
        "start": "yarn workspace @threed/react-app start",
        "compile": "yarn workspace @threed/hardhat compile",
        "deploy": "yarn workspace @threed/hardhat deploy",
        "watch": "yarn workspace @threed/hardhat watch",
        "accounts": "yarn workspace @threed/hardhat accounts",
        "verify": "yarn workspace @threed/hardhat verify",
        "balance": "yarn workspace @threed/hardhat balance",
        "send": "yarn workspace @threed/hardhat send",
        "ipfs": "yarn workspace @threed/react-app ipfs",
        "surge": "yarn workspace @threed/react-app surge",
        "s3": "yarn workspace @threed/react-app s3",
        "ship": "yarn workspace @threed/react-app ship",
        "generate": "yarn workspace @threed/hardhat generate",
        "account": "yarn workspace @threed/hardhat account",
        "mineContractAddress": "cd packages/hardhat && npx hardhat mineContractAddress",
        "wallet": "cd packages/hardhat && npx hardhat wallet",
        "fundedwallet": "cd packages/hardhat && npx hardhat fundedwallet",
        "flatten": "cd packages/hardhat && npx hardhat flatten",
        "clean": "cd packages/hardhat && npx hardhat clean",
        "run-graph-node": "yarn workspace @threed/services run-graph-node",
        "remove-graph-node": "yarn workspace @threed/services remove-graph-node",
        "clean-graph-node": "yarn workspace @threed/services clean-graph-node",
        "graph-prepare": "mustache packages/subgraph/config/config.json packages/subgraph/src/subgraph.template.yaml > packages/subgraph/subgraph.yaml",
        "graph-codegen": "yarn workspace @threed/subgraph graph codegen",
        "graph-build": "yarn workspace @threed/subgraph graph build",
        "graph-create-local": "yarn workspace @threed/subgraph graph create --node http://localhost:8020/ scaffold-eth/your-contract",
        "graph-remove-local": "yarn workspace @threed/subgraph graph remove --node http://localhost:8020/ scaffold-eth/your-contract",
        "graph-deploy-local": "yarn workspace @threed/subgraph graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 scaffold-eth/your-contract",
        "graph-ship-local": "yarn graph-prepare && yarn graph-codegen && yarn graph-deploy-local",
        "deploy-and-graph": "yarn deploy && yarn graph-ship-local",
        "mint": "yarn workspace @threed/hardhat mint",
        "theme": "yarn workspace @threed/react-app theme",
        "watch-theme": "yarn workspace @threed/react-app watch",
        "subgraph:auth": "yarn workspace @threed/subgraph auth",
        "subgraph:codegen": "yarn workspace @threed/subgraph codegen",
        "subgraph:build": "yarn workspace @threed/subgraph build",
        "subgraph:deploy": "yarn workspace @threed/subgraph deploy",
        "vue-app:build": "yarn workspace @threed/vue-app build",
        "vue-app:ipfs": "yarn workspace @threed/vue-app ipfs",
        "vue-app:lint": "yarn workspace @threed/vue-app lint",
        "vue-app:serve": "yarn workspace @threed/vue-app serve"
    },
    "workspaces": {
        "packages": [
            "packages/*"
        ],
        "nohoist": [
            "**/@graphprotocol/graph-ts",
            "**/@graphprotocol/graph-ts/**",
            "**/hardhat",
            "**/hardhat/**",
            "**/hardhat-ts",
            "**/hardhat-ts/**"
        ]
    },
    "dependencies": {
        "pnpm": "7.5.0"
    },
    "keywords": [
        "ethereum",
        "vue",
        "react",
        "uniswap",
        "workspaces",
        "yarn",
        "threedgarden",
        "threedgarden.eth"
    ]
}
